#membuat contoh dictionary
identitas ={
  'nama': 'sarman',
  'usia': 22,
  'asal': 'majene'
}
#menampilkan dictionary dengan perulangan
for i in identitas.items():
   print(i)

for i in identitas:
   print(i)

for i in identitas.values():
   print(i)
#mengupdate salah satu dictionary
identitas ['usia'] = 20
print(identitas)

#menghapus salah satu dictionary
identitas.pop('asal') 
print(identitas)

print(identitas.popitem())

identitas.clear()
print(identitas)

#del identitas
#print(identitas)

#menambahkan dictionary
identitas['status'] = 'mahasiswa'
print(identitas)