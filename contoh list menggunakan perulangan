#membuat contoh list
nama =["mangga", "apel", "alfukat", "pepaya", "strobery"]

#menampilkan list dengan perulangan
for i in nama :
   print(i)

i = 0
while i < len(nama):
   print(nama[i])
   i += 1
    
#mengupdate salah satu list
nama [2] = "pisang"
print(nama)

#menghapus salah satu list
nama.pop()
print(nama)
del nama [1:3]
print(nama)
nama.remove("mangga")

#menambahkan list
nama.append("mangga")
print(nama)

nama.extend(["apel", "anggur"])
print(nama)

nama.insert(3, "anggur")
print(nama)